abstract class Person {
    protected String name;
    protected long aadharNumber;

    public Person(String name, long aadharNumber) {
        this.name = name;
        this.aadharNumber = aadharNumber;
    }

    public abstract void getData();
    public abstract void display();
}

class Student extends Person {
    private int marks;

    public Student(String name, long aadharNumber, int marks) {
        super(name, aadharNumber);
        this.marks = marks;
    }

    @Override
    public void getData() {
    }

    @Override
    public void display() {
        System.out.println("Student Name: " + name);
        System.out.println("Aadhar Number: " + aadharNumber);
        System.out.println("Marks Secured: " + marks);
    }
}

class Faculty extends Person {
    private double salary;

    public Faculty(String name, long aadharNumber, double salary) {
        super(name, aadharNumber);
        this.salary = salary;
    }

    @Override
    public void getData() {
        
    }

    @Override
    public void display() {
        System.out.println("Faculty Name: " + name);
        System.out.println("Aadhar Number: " + aadharNumber);
        System.out.println("Salary: $" + salary);
    }
}

public class Main {
    public static void main(String[] args) {
       
        Student student = new Student("John", 1234567890, 95);
        student.getData(); 
        student.display(); 

      
        Faculty faculty = new Faculty("Prof. Smith", 9876543210, 60000.00);
        faculty.getData(); 
        faculty.display(); 
    }
}
